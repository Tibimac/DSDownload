source 'https://github.com/CocoaPods/Specs'

use_frameworks!

target 'DSDownload-iOS' do
  platform :ios, '12.0'
  pod 'RealmSwift'
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'ObjectMapper'
  pod 'ObjectMapperAdditions/Realm'
  pod 'SynologySwift', :git => 'https://github.com/Thomaslegravier/SynologySwift.git', :branch => 'develop'
end


target 'DSDownload-macOS' do
  platform :osx, '10.14'
  pod 'RealmSwift'
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'ObjectMapper'
  pod 'ObjectMapperAdditions/Realm'
  pod 'SynologySwift', :git => 'https://github.com/Thomaslegravier/SynologySwift.git', :branch => 'develop'
end
